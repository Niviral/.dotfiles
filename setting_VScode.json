{
    "workbench.colorTheme": "Default Dark+",
    "files.associations": {
        "*.py": "python",
        "*.tf": "terraform",
        "*.yml": "yaml"
    },
    "[yaml]": {
    
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced"
    },
    "[python]": {
        "editor.wordBasedSuggestions": false,
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.autoIndent": "advanced"
    },
    "[terraform]": {
            
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced"

    }
}